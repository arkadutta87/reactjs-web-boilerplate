{
  "name": "reactjs-web-boilerplate",
  "version": "1.2.6",
  "repository": "git@github.com:360fy/reactjs-web-boilerplate.git",
  "description": "Boilerplate for react based isomorphic application",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "postinstall": "npm run compile",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.4.6",
    "classnames": "2.2.5",
    "events": "1.1.1",
    "history": "4.4.0",
    "immutable": "3.8.1",
    "jquery": "3.1.1",
    "lodash": "4.17.2",
    "react": "15.4.0",
    "react-addons-css-transition-group": "15.4.0",
    "react-dom": "15.4.0",
    "react-router": "3.0.0",
    "rest": "2.0.0",
    "velocity-animate": "1.3.1",
    "velocity-react": "1.1.11"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.16.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-plugin-react": "6.7.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5",
      "react"
    ]
  }
}
